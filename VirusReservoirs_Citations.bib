  keywords = {Zoonosis,Diagnostic marker,ELISA,Foamy virus,Humoral immunity,Milk-borne infection},
  pages = {123-131},
  file = {/Users/nardus/Zotero/storage/2FVTIY5U/Romen et al. - 2007 - Serological detection systems for identification o.pdf;/Users/nardus/Zotero/storage/7KZ55D9K/S0042682207001535.html}
