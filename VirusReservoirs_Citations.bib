  pages = {327-343},
  file = {/Users/nardus/Zotero/storage/NYQSIINH/Rupprecht, Hanlon, Hemachudha - 2002 - Rabies re-examined.pdf},
  pmid = {12144896}
}

@article{Oura2012,
