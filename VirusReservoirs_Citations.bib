  author = {Monroe, B P and Nakazawa, Y J and Reynolds, M G and Carroll, D S},
  year = {2014},
  keywords = {animal baboon Democratic Republic Congo disease ca},
  pages = {34},
  file = {/Users/nardus/Zotero/storage/F6CGHEPA/document(12).pdf},
