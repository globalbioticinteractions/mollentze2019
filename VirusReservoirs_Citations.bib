@incollection{Lipton2007,
  edition = {2nd},
  title = {Cardioviruses: {{Encephalomyocarditis Virus}} and {{Theiler}}'s {{Murine Encephalomyelitis Virus}}},
  volume = {2},
  isbn = {978-0-12-369454-6},
  abstract = {The cardiovirus genus in the picornaviridae family consists of two groups encephalomyocarditis virus (EMCV) and theiler's murine encephalomyelitis virus (TMEV). The capsids are compact and relatively impermeable to electron-dense salts, precluding elucidation of substructure by electron microscopy. Cardioviruses are more resistant than lipid-containing viruses to chemical and physical agents. Outbreaks of TMEV infection in colony-raised rats are much less common. In nature, TMEV infection is widespread in feral Mus musculus, to a lesser extent in certain species of voles. Virus transmission occurs by the fecal-oral route, with TMEV causing inapparent infections of the intestine, although, even inapparent infections may perturb immune and other physiologic host functions. TMEV infection poses a greater epizootic threat for colony bred mice and rats used in biomedical research. In fact, outbreaks of EMCV infection are unknown in mouse and rat colonies, but experimental infections are usually fatal and provide experimental animal models for myocarditis and diabetes mellitus. \textcopyright{} 2007 Elsevier Inc. All rights reserved.},
