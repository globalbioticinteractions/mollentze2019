  year = {2004},
  keywords = {Arboviruses,Tick-borne viruses,Ticks,Vectors},
  pages = {S221-S245},
  file = {/Users/nardus/Zotero/storage/9TY6W8WF/Labuda and Nuttall - 2004 - Tick-borne viruses.pdf},
