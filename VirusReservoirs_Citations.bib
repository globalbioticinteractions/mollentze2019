  author = {Walker, Peter J. and Widen, Steven G. and Firth, Cadhla and Blasdell, Kim R. and Wood, Thomas G. and Travassos Da Rosa, Amelia P.A. and Guzman, Hilda and Tesh, Robert B. and Vasilakis, Nikos},
