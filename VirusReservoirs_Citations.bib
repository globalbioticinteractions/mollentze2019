  year = {2013},
  keywords = {Phylogeny,Hantavirus,Shrew,Crocidura,Guinea},
  pages = {118-123},
  file = {/Users/nardus/Zotero/storage/G5R76LAW/1-s2.0-S1567134813003122-main.pdf},
